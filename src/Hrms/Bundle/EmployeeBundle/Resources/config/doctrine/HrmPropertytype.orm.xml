<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Hrms\Bundle\EmployeeBundle\Entity\HrmPropertytype" table="hrm_propertytype">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="typeName" type="string" column="type_name" length="255" nullable="false"/>
    <field name="typeDescription" type="string" column="type_description" length="50" nullable="false"/>
    <many-to-one field="company" target-entity="HrmCompany">
      <join-columns>
        <join-column name="company" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
