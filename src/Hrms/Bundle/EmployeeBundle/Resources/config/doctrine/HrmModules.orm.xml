<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Hrms\Bundle\EmployeeBundle\Entity\HrmModules" table="hrm_modules">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="moduleName" type="string" column="module_name" length="50" nullable="false"/>
    <field name="moduleValue" type="string" column="module_value" length="50" nullable="false"/>
    <field name="isactive" type="boolean" column="isactive" nullable="false"/>
    <field name="createDate" type="date" column="create_date" nullable="false"/>
    <field name="orderby" type="integer" column="orderby" nullable="false"/>
  </entity>
</doctrine-mapping>
